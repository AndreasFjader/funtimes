{
  "name": "funtimes",
  "displayName": "FunTimes",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.convertWithOption",
    "onCommand:extension.convertToEpoch",
    "onCommand:extension.convertToUtc",
    "onCommand:extension.convertToLocal"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [],
    "keybindings": [
      {
        "command": "extension.convertWithOption",
        "key": "ctrl+shift+,"
      },
      {
        "command": "extension.convertToEpoch",
        "key": "ctrl+alt+I"
      },
      {
        "command": "extension.convertToUtc",
        "key": "ctrl+alt+U"
      },
      {
        "command": "extension.convertToLocal",
        "key": "ctrl+alt+L"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.84.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "luxon": "^3.4.4"
  }
}
